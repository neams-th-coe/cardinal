[OCCA]
  backend = CPU

[GENERAL]
  stopAt = numSteps
  numSteps = 10
  dt = 0.00125
  polynomialOrder = 3
  writeControl = steps
  writeInterval = 10

[VELOCITY]
  solver = none

[PRESSURE]

[TEMPERATURE]
  conductivity = 2.5
  rhoCp = 1.0
  residualTol = 1.0e-10
  boundaryTypeMap = t, f, I, I, I, I

[OCCA]
  backend = CPU

[GENERAL]
  stopAt = numSteps
  numSteps = 10
  dt = 0.05
  polynomialOrder = 5
  checkpointControl = steps
  checkpointInterval = 10
  scalars = temperature

[FLUID VELOCITY]
  solver = none

[SCALAR TEMPERATURE]
  diffusionCoeff = 10.5
  transportCoeff = 1.0
  residualTol = 1.0e-6
  boundaryTypeMap = t, f, I, I, I, I

[OCCA]
  backend = CPU

[GENERAL]
  stopAt = numSteps
  numSteps = 10
  dt = 0.05
  polynomialOrder = 2
  checkpointControl = steps
  checkpointInterval = 9
  scalars = temperature

[MESH]
  file=pyramid.re2

[FLUID VELOCITY]
  solver = none
  residualTol = 1.0e-6
  boundaryTypeMap = inlet, outlet, wall, wall, wall, wall

[FLUID PRESSURE]
  residualTol = 1.0e-5

[SCALAR TEMPERATURE]
  diffusionCoeff = 10.5
  transportCoeff = 1.0
  residualTol = 1.0e-6
  boundaryTypeMap = t, f, I, I, I, I

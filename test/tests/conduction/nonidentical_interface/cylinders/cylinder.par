[OCCA]
  backend = CPU

[GENERAL]
  stopAt = numSteps
  numSteps = 10
  dt = 0.1
  polynomialOrder = 4
  writeControl = steps
  writeInterval = 50
  scalars = temperature

[FLUID VELOCITY]
  solver = none

[SCALAR TEMPERATURE]
  diffusionCoeff = 2.5
  transportCoeff = 1.0
  residualTol = 1.0e-10
  boundaryTypeMap = t, f, I, I

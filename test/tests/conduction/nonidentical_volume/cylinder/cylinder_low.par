[OCCA]
  backend = CPU

[GENERAL]
  stopAt = numSteps
  numSteps = 10
  dt = 0.1
  polynomialOrder = 3
  writeControl = steps
  writeInterval = 2

[MESH]
  file=cylinder.re2

[VELOCITY]
  solver = none

[PRESSURE]

[TEMPERATURE]
  conductivity = 1.5
  rhoCp = 1.0
  residualTol = 1.0e-5
  boundaryTypeMap = t, I, I

[OCCA]
  backend = CPU

[GENERAL]
  stopAt = numSteps
  numSteps = 10
  dt = 0.1
  polynomialOrder = 3
  writeControl = steps
  writeInterval = 2
  scalars = temperature

[MESH]
  file=cylinder.re2

[FLUID VELOCITY]
  solver = none

[FLUID PRESSURE]

[SCALAR TEMPERATURE]
  diffusionCoeff = 1.5
  transportCoeff = 1.0
  residualTol = 1.0e-5
  boundaryTypeMap = t, I, I

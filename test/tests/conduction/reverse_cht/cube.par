[OCCA]
  backend = CPU

[GENERAL]
  stopAt = numSteps
  numSteps = 10
  dt = 0.02
  polynomialOrder = 1
  writeControl = steps
  writeInterval = 10
  scalars = temperature

[MESH]
  file=../identical_interface/cube/cube.re2

[FLUID VELOCITY]
  solver = none

[FLUID PRESSURE]

[SCALAR TEMPERATURE]
  diffusionCoeff = 1.5
  transportCoeff = 1.0
  residualTol = 1.0e-10
  boundaryTypeMap = I, I, I, t, I, t

[OCCA]
  backend = CPU

[GENERAL]
  stopAt = numSteps
  numSteps = 10
  dt = 0.1
  polynomialOrder = 7
  checkpointControl = steps
  checkpointInterval = 10
  scalars = temperature

[FLUID VELOCITY]
  solver = none

[SCALAR TEMPERATURE]
  diffusionCoeff = 1.5
  transportCoeff = 1.0
  residualTol = 1.0e-5
  boundaryTypeMap = I, t, t, t, t, I

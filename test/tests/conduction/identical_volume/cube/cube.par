[OCCA]
  backend = CPU

[GENERAL]
  stopAt = numSteps
  numSteps = 10
  dt = 0.1
  polynomialOrder = 7
  writeControl = steps
  writeInterval = 10

[VELOCITY]
  solver = none
  residualTol = 1.0e-6
  residualProj = false
  boundaryTypeMap = inlet, outlet, wall, wall, wall, wall

[PRESSURE]
  residualTol = 1.0e-5
  residualProj = false

[TEMPERATURE]
  conductivity = 1.5
  rhoCp = 1.0
  residualTol = 1.0e-5
  residualProj = false
  boundaryTypeMap = I, t, t, t, t, I

[OCCA]
  backend = CPU

[GENERAL]
  stopAt = numSteps
  numSteps = 1
  dt = 5.0e-4
  polynomialOrder = 7
  writeControl = steps
  writeInterval = 2

[VELOCITY]
  viscosity = 2.37e-4
  density = 834.5
  residualTol = 1.0e-6
  boundaryTypeMap = inlet, outlet, wall

[PRESSURE]
  residualTol = 1.0e-5

[TEMPERATURE]
  solver = none

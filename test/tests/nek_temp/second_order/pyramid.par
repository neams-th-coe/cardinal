[OCCA]
  backend = CPU

[GENERAL]
  stopAt = numSteps
  numSteps = 2
  dt = 5.0e-4
  polynomialOrder = 5
  checkpointControl = steps
  checkpointInterval = 1
  scalars = temperature

[FLUID VELOCITY]
  #solver = none    #fluid must be solved to read in bIDs
  boundaryTypeMap = w,w,w,w,w,w,w,w
  rho =1.0
  viscosity = -100

[FLUID PRESSURE]

[SCALAR TEMPERATURE]
  solver = none

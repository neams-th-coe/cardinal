[OCCA]
  backend = CPU

[GENERAL]
  stopAt = numSteps
  numSteps = 2
  dt = 5.0e-4
  polynomialOrder = 3
  checkpointControl = steps
  checkpointInterval = 2
  scalars = temperature

[MESH]
  file = ../../common_inputs/brick2.re2

[FLUID VELOCITY]
  viscosity = 1.0
  density = 1.0
  residualTol = 1.0e-6
  boundaryTypeMap = symy, symy, inlet, outlet, symz, symz

[FLUID PRESSURE]
  residualTol = 1.0e-5

[SCALAR TEMPERATURE]
  solver = none

[OCCA]
  backend = CPU
  deviceNumber = 0

[GENERAL]
  stopAt = numSteps
  numSteps = 7
  dt = 5.0e-4
  polynomialOrder = 1
  writeControl = timeStep
  writeInterval = 2
  extrapolation = OIFS
  targetCFL = 2.0

[PROBLEMTYPE]
  equation = incompNS
  axiSymmetry = no
  swirl = no
  cyclicBoundaries = no
  variableProperties = no
  stressFormulation = no

[MESH]
  motion = none

[VELOCITY]
  viscosity = 1.0
  density = 1.0
  residualTol = 1.0e-6
  residualProj = no
  boundaryTypeMap = symz, symz, inlet, outlet, symy, symy

[PRESSURE]
  residualTol = 1.0e-5
  residualProj = no
  preconditioner = semg_xxt
  writeToFieldFile = yes

[TEMPERATURE]
  residualTol = 1.0e-5
  residualProj - no
  boundaryTypeMap = f, f, f, f, f, f

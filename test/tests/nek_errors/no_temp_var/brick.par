[OCCA]
  backend = CPU

[GENERAL]
  stopAt = numSteps
  numSteps = 1000
  dt = 5.0e-4
  polynomialOrder = 3
  writeControl = steps
  writeInterval = 2

[VELOCITY]
  viscosity = 1.0
  density = 1.0
  residualTol = 1.0e-6
  boundaryTypeMap = symy, symy, inlet, outlet, symz, symz

[PRESSURE]
  residualTol = 1.0e-5

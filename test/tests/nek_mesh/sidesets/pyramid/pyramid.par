[OCCA]
  backend = CUDA
  deviceNumber = 0

[GENERAL]
  stopAt = numSteps
  numSteps = 1000
  dt = 5.0e-4
  polynomialOrder = 3
  writeControl = timeStep
  writeInterval = 2
  extrapolation = OIFS
  targetCFL = 2.0

[PROBLEMTYPE]
  equation = incompNS
  axiSymmetry = no
  swirl = no
  cyclicBoundaries = no
  variableProperties = no
  stressFormulation = no

[MESH]
  motion = none

[VELOCITY]
  viscosity = 1.0
  density = 1.0
  residualTol = 1.0e-6
  residualProj = no
  boundaryTypeMap = inlet, outlet, wall, wall, wall, wall, wall, wall

[PRESSURE]
  residualTol = 1.0e-5
  residualProj = no
  preconditioner = semg_xxt
  writeToFieldFile = yes

[TEMPERATURE]
  solver = none
  residualTol = 1.0e-5
  residualProj - no
  boundaryTypeMap = f, f, f, f, I, I, I, I

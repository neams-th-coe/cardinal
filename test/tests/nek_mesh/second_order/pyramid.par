[OCCA]
  backend = CPU

[GENERAL]
  stopAt = numSteps
  numSteps = 1
  dt = 1
  polynomialOrder = 5
  checkpointControl = steps
  checkpointInterval = 2
  scalars = temperature

[FLUID VELOCITY]
  #solver = none    #fluid must be solved to read in bIDs
  boundaryTypeMap = w,w,w,w,w,w,w,w
  rho =1.0
  viscosity = -100

[SCALAR TEMPERATURE]
  solver = none

[OCCA]
  backend = CPU

[GENERAL]
  stopAt = numSteps
  numSteps = 10
  dt = 5.0e-4
  polynomialOrder = 7
  writeControl = steps
  writeInterval = 2

[VELOCITY]
  viscosity = -880.2742616033756
  density = 1.0
  residualTol = 1.0e-6
  boundaryTypeMap = inlet, outlet, wall

[PRESSURE]
  residualTol = 1.0e-5

[TEMPERATURE]
  solver = none
  residualTol = 1.0e-5
  boundaryTypeMap = t, t, t

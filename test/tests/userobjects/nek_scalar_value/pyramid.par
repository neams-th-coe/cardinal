[OCCA]
  backend = CPU

[GENERAL]
  stopAt = numSteps
  numSteps = 10
  dt = 0.5
  polynomialOrder = 3
  writeControl = steps
  writeInterval = 2

[MESH]
  file = "../../nek_mesh/first_order/pyramid.re2"

[VELOCITY]
  solver = none

[PRESSURE]

[TEMPERATURE]
  residualTol = 1.0e-5
  boundaryTypeMap = f, f, f, f, t, t, I, I

[OCCA]
  backend = CPU

[GENERAL]
  stopAt = numSteps
  numSteps = 1
  dt = 5.0e-4
  polynomialOrder = 1
  writeControl = steps
  writeInterval = 1

[MESH]
  file=../../nek_output/pyramid.re2

[VELOCITY]
  solver = none

[PRESSURE]

[TEMPERATURE]
  conductivity = 1
  rhoCp = 1
  boundaryTypeMap = t, I, I, I, I, I, I, I
  residualTol = 1e-6

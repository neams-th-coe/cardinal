[OCCA]
  backend = CPU

[GENERAL]
  stopAt = numSteps
  numSteps = 3
  dt = 5.0e-4
  polynomialOrder = 5
  writeControl = steps
  writeInterval = 1

[VELOCITY]
  solver = none
  viscosity = 1.0
  density = 834.5
  residualTol = 1.0e-6
  residualProj = false
  boundaryTypeMap = wall, wall, wall, wall, wall, wall

[PRESSURE]
  residualTol = 1.0e-5
  residualProj = false

[TEMPERATURE]
  solver = none
  residualTol = 1.0e-5
  residualProj  = no
  boundaryTypeMap = f, f, f, f, f, f

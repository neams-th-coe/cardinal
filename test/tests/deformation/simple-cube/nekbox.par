[OCCA]
  backend = CPU

[GENERAL]
  stopAt = numSteps
  numSteps = 50
  dt = 0.1
  polynomialOrder = 7
  writeControl = timeStep
  writeInterval = 10
  extrapolation = subCycling

[MESH]
  solver = user

[VELOCITY]
  solver = none
  residualTol = 1.0e-6
  residualProj = no
  boundaryTypeMap = inlet, outlet, wall, wall, wall, wall

[PRESSURE]
  residualTol = 1.0e-5
  residualProj = no
  writeToFieldFile = yes

[TEMPERATURE]
  conductivity = 1.0
  rhoCp = 0.0
  residualTol = 1.0e-5
  residualProj = no
  boundaryTypeMap = t, t, t, t, t, t
